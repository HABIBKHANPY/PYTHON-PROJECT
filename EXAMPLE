print("Class and Object")

class Bike:
    name=""
    gear=0

bike1=Bike()

bike1.gear=11
bike1.name="Honda"

print(f"Name: {bike1.name}, Gear:{bike1.gear}")

bike2=Bike()

bike2.gear=5
bike2.name="Mountain Bike"

print(f"Name: {bike2.name}, Gear:{bike2.gear}")


print("Class and Object Example 2")


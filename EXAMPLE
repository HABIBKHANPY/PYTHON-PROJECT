class Human:
  attr1="Human"
  attr2="Student"

  def fun (self):
   print("I'm a", self.attr1)
   print("I'm a", self.attr2)


Salman=Human()

print(Salman.attr1)
Salman.fun()


class Class:
  attr1="Human"
  attr2="Student"

  def fun (self):
   print("I'm a", self.attr1)
   print("I'm a", self.attr2)


Salman=Class()

print(Salman.attr1)
Salman.fun()



class Five:
    name=""
    age=0

student1= Five()

student1.name="Salman"
student1.age=12

print(f"Name:{student1.name}, Age:{student1.age}")


student2= Five()

student2.name="Ahmed"
student2.age=13

print(f"Name:{student2.name}, Age:{student2.age}")

student3=Five()


student3.name="Ibrahim"
student3.age=14


print(f"Name:{student3.name}, Age{student3.age}")

